{
    "name": "@nktkas/hyperliquid",
    "version": "0.25.0-beta.2",
    "exports": {
        ".": "./src/mod.ts",
        "./schemas": "./src/schemas/mod.ts",
        "./signing": "./src/signing/mod.ts"
    },
    "imports": {
        "@msgpack/msgpack": "npm:@msgpack/msgpack@^3.1.2",
        "@noble/hashes/sha3": "npm:@noble/hashes@^2.0.0/sha3.js",
        "@noble/secp256k1": "npm:@noble/secp256k1@^3.0.0",
        "@std/cli/parse-args": "jsr:@std/cli@^1.0.22/parse-args",
        "valibot": "npm:valibot@1.1.0"
    },
    "fmt": {
        "indentWidth": 4,
        "lineWidth": 120
    },
    "lint": {
        "rules": {
            "exclude": ["no-slow-types"]
        }
    },
    "publish": {
        "exclude": [
            ".github",
            "build",
            "tests"
        ]
    },
    "license": "MIT",
    "lock": false,
    "tasks": {
        "build:bundle": "deno run --allow-env --allow-read --allow-write --allow-run build/bundle.ts",
        "build:npm": "deno run --allow-env --allow-read --allow-write --allow-run build/npm.ts"
    }
}
