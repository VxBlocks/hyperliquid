{
    "name": "@nktkas/hyperliquid",
    "version": "0.24.3",
    "exports": {
        ".": "./mod.ts",
        "./schemas": "./src/schemas/mod.ts",
        "./signing": "./src/signing/mod.ts"
    },
    "imports": {
        "@msgpack/msgpack": "npm:@msgpack/msgpack@^3.1.2",
        "@noble/hashes/sha3": "npm:@noble/hashes@^1.8.0/sha3",
        "@noble/secp256k1": "npm:@noble/secp256k1@^2.3.0",
        "valibot": "npm:valibot@1.1.0"
    },
    "fmt": {
        "indentWidth": 4,
        "lineWidth": 120
    },
    "lint": {
        "rules": {
            "exclude": ["no-slow-types"]
        }
    },
    "publish": {
        "exclude": [
            ".github",
            "build",
            "tests"
        ]
    },
    "license": "MIT",
    "lock": false,
    "tasks": {
        "bundle": "deno run --allow-env --allow-read --allow-write --allow-run build/bundle.ts",
        "build-npm": "deno run --allow-env --allow-read --allow-write --allow-run build/npm.ts"
    }
}
